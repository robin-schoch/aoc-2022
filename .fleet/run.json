{
    "configurations": [
        {
            "name": "Build aoc-2022",
            "type": "gradle",
            "tasks": [
                ":classes"
            ],
            "initScripts": {
                "flmapper": "ext.mapPath = { path -> null }",
                "Build aoc-2022": "System.setProperty('org.gradle.java.compile-classpath-packaging', 'true')\n"
            }
        },
        {
            "name": "Every Day",
            "type": "jps-run",
            "dependsOn": [
                "Build aoc-2022"
            ],
            "mainClass": "AdventOfCodeSolutionKt",
            "module": "aoc-2022.main",
            "options": [
                "-Dfile.encoding=UTF-8",
                "-Dsun.stdout.encoding=UTF-8",
                "-Dsun.stderr.encoding=UTF-8"
            ]
        },
